{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", "tslint-react", "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "interface-name": ["never-prefix"],
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-console": [
            false
        ],
        "no-var-requires": false,
        "quotemark": [true, "double", "jsx-double"],
        "no-shadowed-variable": false
    },
    "rulesDirectory": []
}