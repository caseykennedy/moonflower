{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "interface-name": ["never-prefix"],
        "semicolon": [true, "never"],
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-console": [
            false
        ],
        "quotemark": [true, "double", "jsx-double"],
        "no-shadowed-variable": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}